{
  "pasteImage.basePath": "${currentFileDir}",
  "pasteImage.path": "${currentFileDir}/assets/${currentFileNameWithoutExt}",
  "pasteImage.defaultName": "mmss",
  "pasteImage.insertPattern": "<center>\n\n${imageSyntaxPrefix}.${imageFilePath}${imageSyntaxSuffix}\n</center>\n",
  "cSpell.words": [
    "Anotacionais",
    "COCOMO",
    "criacionais",
    "Ishikawa"
  ],
  "markdownlint.config": { "MD033": false, "MD045": false, "MD041": false, "MD024": false }
}
